<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- text input layout -->
    <style name="textInputLayoutStyle" parent="Theme.MaterialComponents">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin_20dp</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="boxStrokeColor">@color/colorPrimary</item>
        <item name="hintTextColor">@color/colorPrimary</item>
        <item name="boxStrokeWidth">@dimen/height_05dp</item>
        <item name="boxStrokeWidthFocused">@dimen/height_05dp</item>
    </style>

    <!-- app button -->
    <style name="appCompatButtonPrimary" parent="font14_263238_medium">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_50dp</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="textAllCaps">true</item>
        <item name="android:background">@drawable/bg_primary_selector</item>
    </style>

    <style name="BottomSheetDialogSlideDown">
        <item name="android:windowExitAnimation">@anim/slide_down</item>
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
    </style>

    <style name="BottomSheetDialog" parent="@style/Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:colorAccent">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <!-- dialog animation -->
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/scale_in</item>
        <item name="android:windowExitAnimation">@anim/scale_out</item>
    </style>


