<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="pf_key_button" format="reference|integer"/>
    <attr name="pf_delete_button" format="reference|integer"/>
    <attr name="pf_code_view" format="reference|integer"/>

    <declare-styleable name="RealtimeBlurView">
        <attr name="realtimeBlurRadius" format="dimension"/>
        <attr name="realtimeDownsampleFactor" format="float"/>
        <attr name="realtimeOverlayColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="BlurLayout">
        <attr name="blk_downscaleFactor" format="float" />
        <attr name="blk_blurRadius" format="integer" />
        <attr name="blk_fps" format="integer" />
        <attr name="blk_cornerRadius" format="dimension" />
        <attr name="blk_alpha" format="float" />
    </declare-styleable>

    <integer name="default_blur_radius">15</integer>
    <integer name="default_downsample_factor">8</integer>
    <color name="default_overlay_color">#AAFFFFFF</color>

    <declare-styleable name="PxBlurringView">
        <attr name="blurRadius" format="integer"/>
        <attr name="downsampleFactor" format="integer"/>
        <attr name="overlayColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="PinView">
        <attr name="titleName" format="string" />
        <attr name="titleTextColor" format="color" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="dotProgressColor" format="color" />
        <attr name="dotUnProgressColor" format="color" />
        <attr name="dotRadius" format="dimension" />
        <attr name="numbersTextSize" format="dimension" />
        <attr name="numbersTextColor" format="color" />
        <attr name="deleteButtonColor" format="color" />
        <attr name="clearButtonColor" format="color" />
        <attr name="errorMessageText" format="string" />
        <attr name="errorMessageTextSize" format="dimension" />
        <attr name="errorMessageColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ValidateButton">
        <attr name="buttonText" format="string"/>
        <attr name="buttonTextColor" format="color"/>
        <attr name="buttonBackground" format="reference"/>
        <attr name="is_active" format="boolean"/>
        <attr name="textAllCaps" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="AvatarBehavior">
        <attr name="startYPosition" format="dimension"/>
        <attr name="startXPosition" format="dimension"/>
        <attr name="endYPosition" format="dimension"/>
        <attr name="endXPosition" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="StepView">
        <attr name="sv_selectedCircleColor" format="color" />
        <attr name="sv_selectedCircleRadius" format="dimension" />
        <attr name="sv_selectedTextColor" format="color" />
        <attr name="sv_selectedStepNumberColor" format="color" />
        <attr name="sv_stepNumberTextSize" format="dimension" />
        <attr name="sv_doneStepMarkColor" format="color" />
        <attr name="sv_doneCircleColor" format="color" />
        <attr name="sv_doneCircleRadius" format="dimension" />
        <attr name="sv_doneTextColor" format="color" />
        <attr name="sv_nextTextColor" format="color" />
        <attr name="sv_nextStepLineColor" format="color" />
        <attr name="sv_doneStepLineColor" format="color" />
        <attr name="sv_stepPadding" format="dimension" />
        <attr name="sv_textPadding" format="dimension" />
        <attr name="sv_stepLineWidth" format="dimension" />
        <attr name="sv_textSize" format="dimension" />
        <attr name="sv_background" format="color" />
        <attr name="sv_animationDuration" format="integer" />
        <attr name="sv_stepViewStyle" format="reference" />
        <attr name="sv_animationType" format="enum">
            <enum name="Line" value="0" />
            <enum name="Circle" value="1" />
            <enum name="All" value="2" />
            <enum name="None" value="3" />
        </attr>
        <attr name="sv_stepsNumber" format="integer" />
        <attr name="sv_steps" format="reference" />
        <attr name="sv_typeface" format="reference" />
        <attr name="sv_nextStepCircleEnabled" format="boolean" />
        <attr name="sv_nextStepCircleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="JellyToggleButton">

        <attr name="jtbLeftBackgroundColor" format="color|reference"/>
        <attr name="jtbRightBackgroundColor" format="color|reference"/>
        <attr name="jtbLeftThumbColor" format="color|reference"/>
        <attr name="jtbRightThumbColor" format="color|reference"/>
        <attr name="jtbLeftTextColor" format="color|reference"/>
        <attr name="jtbRightTextColor" format="color|reference"/>
        <attr name="jtbLeftTextTypeface" format="string"/>
        <attr name="jtbRightTextTypeface" format="string"/>
        <attr name="jtbLeftTextSize" format="dimension|reference"/>
        <attr name="jtbRightTextSize" format="dimension|reference"/>
        <attr name="jtbLeftText" format="string|reference"/>
        <attr name="jtbRightText" format="string|reference"/>

        <attr name="jtbTextMarginLeft" format="dimension|reference"/>
        <attr name="jtbTextMarginRight" format="dimension|reference"/>
        <attr name="jtbTextMarginTop" format="dimension|reference"/>
        <attr name="jtbTextMarginBottom" format="dimension|reference"/>
        <attr name="jtbTextMarginCenter" format="dimension|reference"/>

        <attr name="jtbThumbRadius" format="dimension|reference"/>

        <attr name="jtbBackgroundMeasureRatioValue" format="float|reference"/>
        <attr name="jtbBackgroundRadius" format="dimension|reference"/>

        <attr name="jtbColorChangeType" format="enum">
            <enum name="rgb" value="0"/>
            <enum name="hsv" value="1"/>
        </attr>

        <attr name="jtbJelly" format="enum">
            <enum name="itself" value="0"/>
            <enum name="lazy_tremble_head_fatty" value="1"/>
            <enum name="lazy_tremble_head_slim_jim" value="2"/>
            <enum name="lazy_tremble_tail_fatty" value="3"/>
            <enum name="lazy_tremble_tail_slim_jim" value="4"/>
            <enum name="lazy_tremble_body_fatty" value="5"/>
            <enum name="lazy_tremble_body_slim_jim" value="6"/>
            <enum name="lazy_stiff_fatty" value="7"/>
            <enum name="lazy_stiff_slim_jim" value="8"/>
            <enum name="active_tremble_head_fatty" value="9"/>
            <enum name="active_tremble_head_slim_jim" value="10"/>
            <enum name="active_tremble_tail_fatty" value="11"/>
            <enum name="active_tremble_tail_slim_jim" value="12"/>
            <enum name="active_tremble_body_fatty" value="13"/>
            <enum name="active_tremble_body_slim_jim" value="14"/>
            <enum name="active_stiff_fatty" value="15"/>
            <enum name="active_stiff_slim_jim" value="16"/>
            <enum name="random" value="17"/>
        </attr>

        <attr name="jtbEaseType" format="enum">
            <enum name="ease_in_sine" value="0"/>
            <enum name="ease_out_sine" value="1"/>
            <enum name="ease_in_out_sine" value="2"/>
            <enum name="ease_in_quad" value="3"/>
            <enum name="ease_out_quad" value="4"/>
            <enum name="ease_in_out_quad" value="5"/>
            <enum name="ease_in_cubic" value="6"/>
            <enum name="ease_out_cubic" value="7"/>
            <enum name="ease_in_out_cubic" value="8"/>
            <enum name="ease_in_quart" value="9"/>
            <enum name="ease_out_quart" value="10"/>
            <enum name="ease_in_out_quart" value="11"/>
            <enum name="ease_in_quint" value="12"/>
            <enum name="ease_out_quint" value="13"/>
            <enum name="ease_in_out_quint" value="14"/>
            <enum name="ease_in_expo" value="15"/>
            <enum name="ease_out_expo" value="16"/>
            <enum name="ease_in_out_expo" value="17"/>
            <enum name="ease_in_circ" value="18"/>
            <enum name="ease_out_circ" value="19"/>
            <enum name="ease_in_out_circ" value="20"/>
            <enum name="ease_in_back" value="21"/>
            <enum name="ease_out_back" value="22"/>
            <enum name="ease_in_out_back" value="23"/>
            <enum name="ease_in_elastic" value="24"/>
            <enum name="ease_out_elastic" value="25"/>
            <enum name="ease_in_out_elastic" value="26"/>
            <enum name="ease_in_bounce" value="27"/>
            <enum name="ease_out_bounce" value="28"/>
            <enum name="ease_in_out_bounce" value="29"/>
            <enum name="linear" value="30"/>
        </attr>

        <attr name="jtbDuration" format="integer|reference"/>

        <attr name="jtbTouchMoveRatioValue" format="float|reference"/>
        <attr name="jtbBezierControlValue" format="float|reference"/>
        <attr name="jtbStretchDistanceRatioValue" format="float|reference"/>
        <attr name="jtbBezierScaleRatioValue" format="float|reference"/>

        <attr name="jtbMoveToSameStateCallListener" format="boolean|reference"/>
        <attr name="jtbDraggable" format="boolean|reference"/>

    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="mfl_onclickLoading"
            format="boolean"/>
        <attr name="mfl_duration"
            format="integer"/>
        <attr name="mfl_repeat"
            format="integer"/>
    </declare-styleable>
    <declare-styleable name="CircleFillView" >
        <attr name="fillColor" format="color" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeWid" format="float" />
        <attr name="value" format="integer" />
    </declare-styleable>

    <!--  drag view  -->
    <declare-styleable name="DraggableImageView">
        <attr name="sticky" format="enum">
            <enum name="x" value="1"/>
            <enum name="y" value="2"/>
            <enum name="xy" value="3"/>
        </attr>
        <attr name="animate" format="boolean"/>
    </declare-styleable>

</resources>