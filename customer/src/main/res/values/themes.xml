<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BNKC_LAOS" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ToggleSwitchTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorAccent">@color/colorPrimary</item>
    </style>
    <style name="PFLockScreenDeleteButtonStyle">
        <item name="android:background">@drawable/selector_pin_key</item>
    </style>

    <style name="PFLockScreenCodeStyle">
        <item name="android:button">@drawable/selector_pin_num</item>
    </style>

    <style name="PFLockScreenButtonStyle" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="android:textSize">@dimen/font_key_code</item>
        <item name="android:textColor">@color/color_ffffff</item>
        <item name="android:fontFamily">@font/rubik_medium</item>
        <item name="android:background">@drawable/selector_pin_key</item>
    </style>

    <style name="SettingSwitch" parent="Theme.AppCompat.Light">
        <!-- colorPrimary is used for the default action bar background -->
        <item name="colorPrimary">#D7191F</item>

        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">#D7191F</item>

        <!-- colorAccent is used as the default value for colorControlActivated,
             which is used to tint widgets -->
        <item name="colorAccent">#D7191F</item>

        <item name="track">@drawable/switch_track_selector</item>
        <item name="android:thumb">@drawable/switch_thumb_selector</item>
        <!-- You can also set colorControlNormal, colorControlActivated
             colorControlHighlight, and colorSwitchThumbNormal. -->
    </style>

